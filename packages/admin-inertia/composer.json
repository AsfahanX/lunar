{
    "name": "lunarphp/admin-inertia",
    "description": "Admin Hub for Lunar. A modern headless e-commerce solution for Laravel PHP framework.",
    "keywords": ["lunar", "laravel", "ecommerce", "e-commerce", "headless", "store", "shop", "cart"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
          "name": "Lunar",
          "homepage": "https://lunarphp.io/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lunar\\Hub\\Inertia\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lunar\\Hub\\Inertia\\AdminHubServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "inertiajs/inertia-laravel": "^0.6.9",
        "lunarphp/admin": "self.version",
        "tightenco/ziggy": "^1.6"
    }
}
